var n=0;var timing;var allItemList=accessoriesClassifyList;var recommend=itemCommandList;var hotSelling=hotStoItemCommandList;var imgId="";var classification="";var plpImageList=new Array();$(".accessories-products,.recommend-products,.hotSelling-rotation").on("mouseenter",function(){var a=new Array();plpImageList=new Array();var d=0;imgId=$(this).find("img").attr("id");var e=parseInt(imgId.substring(11,imgId.length));classification=$(".num").find(".active").html();var c=allItemList[classification];for(var b=0;b<c.length;b++){if(c[b].id==e){a=c[b].allImagesList;continue}}for(var b=0;b<a.length;b++){plpImageList[d]=a[b];d++}if(plpImageList.length>1){accessoriesRotating()}else{return}});$(".accessories-products , .recommend-products , .hotSelling-rotation").on("mouseleave",function(){var b=$(this);var a=b.find("img").attr("id");stopRotating(b,plpImageList,a)});function picture(){var a=plpImageList.length;n++;if(n>=a){n=0}lazyImage($("#"+imgId),imgbase+plpImageList[n])}function accessoriesRotating(){timing=setInterval("picture()",500)}function stopRotating(b,c,a){var d=b.find("img").attr("src");window.clearInterval(timing);if(c.length==0){lazyImage($("#"+a),d)}else{lazyImage($("#"+a),imgbase+c[0])}n=0}function lazyImage(b,a){b.attr(" lazy-src",a)};