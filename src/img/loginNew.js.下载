function loginClick(b){var a=base+"/zeus/member/getAuthUrl?source="+b+"&operation=userInfo";loxia.asyncXhrGet(a,null,{success:function(c){if("20002500"==c.statusCode){window.location.href=c.data.authUrl}else{console.log(c.message)}}},{error:function(c){console.log(c.message)}})}$(function(){var a=getQueryString("returnUrl");loxia.asyncXhrGet("/zeus/member/loginTypes",{returnUrl:a},{success:function(b){if(b.statusCode=="20002506"){if(b.data.loginTypes.toString().indexOf("WECHAT")>=0){$(".wechat").removeClass("none")}if(b.data.loginTypes.toString().indexOf("ALIPAY")>=0){$(".alipay").removeClass("none")}}}},{error:function(b){}})});function IsPC(){var a=window.navigator.userAgent;var d=["Android","iPhone","SymbianOS","Windows Phone","iPad","iPod"];var b=true;for(var c=0;c<d.length;c++){if(a.indexOf(d[c])>0){b=false;break}}return b}function getQueryString(a){var b=new RegExp("(^|&)"+a+"=([^&]*)(&|$)","i");var c=window.location.search.substr(1).match(b);if(c!=null){return unescape(c[2])}return null};